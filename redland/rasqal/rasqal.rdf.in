  <Project>
    <name>Rasqal</name>
    <shortdesc xml:lang="en">Rasqal RDF Query Library.</shortdesc>
    <homepage rdf:resource="http://librdf.org/rasqal/"/>
    <created>2003-08-12</created>

    <description xml:lang="en">Rasqal is a library providing
    full support for querying Resource Description Framework (RDF)
    including parsing query syntaxes, constructing the queries,
    executing them and returning result formats.  It supports
    parsing and executing the SPARQL Query language.</description>

    <maintainer>
      <foaf:Person>
        <foaf:name>Dave Beckett</foaf:name>
        <foaf:homepage rdf:resource="http://www.dajobe.org/"/>
        <foaf:mbox_sha1sum>970987f991961f2553a1bf2574166fa29befbccb</foaf:mbox_sha1sum>
      </foaf:Person>
    </maintainer>

    <release>
